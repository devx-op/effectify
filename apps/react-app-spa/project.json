{
  "$schema": "./../../node_modules/nx/schemas/project-schema.json",
  "name": "@effectify/react-app-spa",
  "projectType": "application",
  "sourceRoot": "apps/react-app-spa/src",
  "targets": {
    "dev:watch": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["vite", "nx run @effectify/react-app-spa:watch-deps "],
        "cwd": "apps/react-app-spa",
        "parallel": true
      }
    },
    "dev:server:watch": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "nx serve @effectify/node-auth-app",
          "vite",
          "nx run @effectify/react-app-spa:watch-deps "
        ],
        "cwd": "apps/react-app-spa",
        "parallel": true
      }
    },
    "lint": {
      "executor": "nx-oxlint:lint",
      "options": {
        "lintFilePatterns": [
          "apps/react-app-spa/**/*.{ts,tsx,js,jsx}",
          "!apps/react-app-spa/src/routeTree.gen.ts"
        ]
      }
    }
  },
  "tags": ["solid-start"]
}
