{
  "name": "@effectify/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "lint-staged": "lint-staged --allow-empty",
    "clean": "rm -rf node_modules dist pnpm-lock.yaml && pnpm store prune & find . -type d \\( -name 'node_modules' -o -name 'dist' -o -name '.output' -o -name '.vinxi' \\) -prune -exec rm -rf {} + & wait",
    "prepare": "husky && ts-patch install -s"
  },
  "lint-staged": {
    "{apps,packages}/**/*.{js,jsx,ts,tsx,json,jsonc,css,scss,md,mdx}": [
      "pnpm exec ultracite format"
    ]
  },
  "devDependencies": {
    "@biomejs/biome": "2.1.2",
    "@effect/language-service": "catalog:",
    "@nx/js": "21.3.2",
    "@nx/node": "21.3.2",
    "@nx/vite": "21.3.2",
    "@nx/web": "21.3.2",
    "@types/node": "catalog:",
    "@vitest/ui": "^3.0.0",
    "husky": "^9.1.7",
    "jiti": "2.4.2",
    "lint-staged": "^16.1.2",
    "nx": "21.3.2",
    "ts-patch": "^3.3.0",
    "tslib": "^2.3.0",
    "typescript": "5.8.3",
    "ultracite": "5.0.48",
    "verdaccio": "^6.0.5",
    "vite": "catalog:",
    "vitest": "^3.0.0"
  },

  "nx": {
    "includedScripts": [],
    "targets": {
      "local-registry": {
        "executor": "@nx/js:verdaccio",
        "options": {
          "port": 4873,
          "config": ".verdaccio/config.yml",
          "storage": "tmp/local-registry/storage"
        }
      }
    }
  },
  "trustedDependencies": [
    "@biomejs/biome",
    "@parcel/watcher",
    "@swc/core",
    "@tailwindcss/oxide",
    "better-sqlite3",
    "core-js",
    "esbuild",
    "msgpackr-extract",
    "nx"
  ],
  "dependencies": {}
}
