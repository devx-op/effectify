{
  "name": "@effectify/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "lint-staged": "lint-staged --allow-empty",
    "clean": "rm -rf node_modules dist pnpm-lock.yaml && pnpm store prune & find . -type d \\( -name 'node_modules' -o -name 'dist' -o -name '.output' -o -name '.vinxi' -o -name '.cache' -o -name '.expo' -o -name '.nx' \\) -prune -exec rm -rf {} + & wait",
    "prepare": "husky && ts-patch install -s",
    "build:affected": "pnpm nx affected --target=build",
    "test:affected": "pnpm nx affected --target=test",
    "typecheck:affected": "pnpm nx affected --target=typecheck",
    "publish:affected": "pnpm nx publish-affected",
    "release:affected": "pnpm nx release-affected",
    "release:affected:dry-run": "pnpm nx release-affected-dry-run"
  },
  "lint-staged": {
    "{apps,packages}/**/*.{js,jsx,ts,tsx,json,jsonc,css,scss,md,mdx}": [
      "pnpm exec ultracite format"
    ]
  },
  "devDependencies": {
    "@biomejs/biome": "catalog:",
    "@effect/language-service": "catalog:",
    "@nx/js": "catalog:",
    "@nx/node": "catalog:",
    "@nx/react": "catalog:",
    "@nx/vite": "catalog:",
    "@nx/web": "catalog:",
    "@react-router/dev": "catalog:",
    "@swc-node/register": "catalog:",
    "@swc/core": "catalog:",
    "@swc/helpers": "catalog:",
    "@types/node": "catalog:",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "@vitejs/plugin-react": "catalog:",
    "@vitest/ui": "catalog:",
    "husky": "catalog:",
    "jiti": "catalog:",
    "jsdom": "catalog:",
    "lint-staged": "catalog:",
    "nx": "catalog:",
    "prettier": "catalog:",
    "ts-patch": "catalog:",
    "tslib": "catalog:",
    "typescript": "catalog:",
    "ultracite": "catalog:",
    "verdaccio": "catalog:",
    "vite": "catalog:",
    "vitest": "catalog:"
  },
  "pnpm": {
    "overrides": {
      "@types/react": "19.1.13",
      "@types/react-dom": "19.1.9"
    },
    "onlyBuiltDependencies": [
      "@biomejs/biome",
      "@parcel/watcher",
      "@swc/core",
      "@tailwindcss/oxide",
      "better-sqlite3",
      "core-js",
      "esbuild",
      "msgpackr-extract",
      "nx",
      "sharp"
    ]
  },
  "packageManager": "pnpm@10.14.0",
  "dependencies": {
    "@react-router/node": "catalog:",
    "@react-router/serve": "catalog:",
    "isbot": "catalog:",
    "react": "catalog:",
    "react-dom": "catalog:",
    "react-router": "catalog:"
  }
}
