  "nx": {
    "includedScripts": [],
    "targets": {
      "local-registry": {
        "executor": "@nx/js:verdaccio",
        "options": {
          "port": 4873,
          "config": ".verdaccio/config.yml",
          "storage": "tmp/local-registry/storage"
        }
      }
    }
  },
  "trustedDependencies": [
    "@biomejs/biome",
    "@parcel/watcher",
    "@swc/core",
    "@tailwindcss/oxide",
    "better-sqlite3",
    "core-js",
