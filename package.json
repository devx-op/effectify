{
  "name": "@effectify/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "postinstall": "node patch-workspaces.js",
    "format": "biome format --write ./",
    "lint": "biome lint ./",
    "lint:fix": "biome check --write ./",
    "lint-staged": "lint-staged --allow-empty",
    "clean": "rm -rf node_modules dist bun.lock && bun pm cache rm & find . -type d \\( -name 'node_modules' -o -name 'dist' -o -name '.output' -o -name '.vinxi' \\) -prune -exec rm -rf {} + & wait",
    "prepare": "husky && ts-patch install -s"
  },
  "lint-staged": {
    "{apps,packages}/**/*.{ts,tsx,test.ts}": [
      "bun run format",
      "bun run lint",
      "bun run lint:fix"
    ]
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@effect/language-service": "catalog:",
    "@nx/js": "21.3.2",
    "@nx/node": "21.3.2",
    "@nx/vite": "21.3.2",
    "@nx/web": "21.3.2",
    "@types/node": "18.16.9",
    "@vitest/ui": "^3.0.0",
    "husky": "^9.1.7",
    "jiti": "2.4.2",
    "lint-staged": "15.5.1",
    "nx": "21.3.2",
    "prettier": "^2.6.2",
    "ts-patch": "^3.3.0",
    "tslib": "^2.3.0",
    "typescript": "5.8.3",
    "verdaccio": "^6.0.5",
    "vite": "catalog:",
    "vitest": "^3.0.0"
  },
  "workspaces": {
    "packages": [
      "apps/*",
      "packages/*",
      "packages/solid/*",
      "packages/react/*",
      "packages/node/*",
      "packages/chat/*",
      "packages/shared/*"
    ],
    "catalog": {
      "@tanstack/solid-query": "5.83.0",
      "@kobalte/core": "0.13.10",
      "effect": "3.17.1",
      "@effect/platform": "0.89.0",
      "@effect/platform-node": "0.94.0",
      "@effect/language-service": "^0.23.3",
      "solid-js": "1.9.7",
      "class-variance-authority": "0.7.1",
      "clsx": "2.1.1",
      "tailwind-merge": "3.3.1",
      "tailwindcss-animate": "1.0.7",
      "lucide-solid": "0.523.0",
      "@corvu/drawer": "0.2.4",
      "better-auth": "1.3.3",
      "better-sqlite3": "11.10.0",
      "@types/better-sqlite3": "7.6.13",
      "@tailwindcss/vite": "4.1.11",
      "@tailwindcss/postcss": "^4.1.11",
      "@tanstack/router-plugin": "1.129.8",
      "@tanstack/solid-router": "1.127.0",
      "@tanstack/solid-start": "1.129.8",
      "@tanstack/solid-form": "^1.12.3",
      "@tanstack/solid-router-devtools": "1.127.0",
      "@tanstack/solid-query-devtools": "^5.83.0",
      "@tanstack/react-query": "^5.83.0",
      "@types/react": "^19.1.8",
      "react": "^19.1.0",
      "react-dom": "^19.1.0",
      "@radix-ui/react-slot": "1.2.3",
      "@radix-ui/react-label": "2.1.7",
      "@radix-ui/react-form": "0.1.7",
      "@tanstack/react-form": "^1.0.0",
      "@tanstack/react-query-devtools": "^5.83.0",
      "@tanstack/react-router": "^1.129.8",
      "@tanstack/react-router-devtools": "^1.129.8",
      "@testing-library/dom": "^10.4.0",
      "@testing-library/react": "^16.2.0",
      "jsdom": "^26.0.0",
      "vitest": "^3.0.5",
      "web-vitals": "^4.2.4",
      "@types/react-dom": "^19.1.6",
      "@vitejs/plugin-react": "^4.6.0",
      "globals": "^16.2.0",
      "tailwindcss": "4.1.11",
      "lucide-react": "^0.525.0",
      "tw-animate-css": "^1.3.5",
      "autoprefixer": "^10.4.21",
      "@types/validator": "13.15.2",
      "validator": "13.15.15",
      "globby": "^14.1.0",
      "postcss": "^8.5.6",
      "vite-tsconfig-paths": "^5.1.4",
      "@biomejs/biome": "1.9.4",
      "typescript": "5.8.3",
      "tslib": "^2.8.1",
      "vite": "7.0.6",
      "vite-plugin-solid": "2.11.7",
      "vite-plugin-dts": "^4.5.4"
    }
  },
  "nx": {
    "includedScripts": [],
    "targets": {
      "local-registry": {
        "executor": "@nx/js:verdaccio",
        "options": {
          "port": 4873,
          "config": ".verdaccio/config.yml",
          "storage": "tmp/local-registry/storage"
        }
      }
    }
  },
  "trustedDependencies": [
    "@biomejs/biome",
    "@parcel/watcher",
    "@swc/core",
    "@tailwindcss/oxide",
    "better-sqlite3",
    "core-js",
    "esbuild",
    "msgpackr-extract",
    "nx"
  ],
  "dependencies": {}
}
