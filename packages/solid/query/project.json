{
  "name": "@effectify/solid-query",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/solid/query/src",
  "projectType": "library",
  "tags": ["solid"],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "main": "packages/solid/query/src/index.ts",
        "outputPath": "packages/solid/query/dist",
        "tsConfig": "packages/solid/query/tsconfig.lib.json"
      },
      "configurations": {
        "development": {
          "minify": false
        },
        "production": {
          "minify": true
        }
      }
    },
    "lint": {
      "executor": "nx-oxlint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/solid/query/**/*.{ts,tsx,js,jsx}"]
      }
    }
  }
}
