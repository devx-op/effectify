      "source": "./src/lib/components/*.ts",
      "types": "./dist/lib/components/*.d.ts",
      "import": "./dist/components/*.js"
    },
    "./preset": "./tailwind.config.cjs",
    "./css": "./src/lib/app.css"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "react": "catalog:",
    "react-dom": "catalog:"
  },
  "devDependencies": {
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "@vitejs/plugin-react": "catalog:",
    "globals": "catalog:",
    "typescript": "catalog:",
    "vite": "catalog:"
  }
}
