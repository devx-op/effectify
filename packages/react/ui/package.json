{
  "name": "@effectify/react-ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "license": "MIT",
  "files": ["dist"],
  "exports": {
    "./utils/*": {
      "source": "./src/lib/utils/*.ts",
      "types": "./dist/lib/utils/*.d.ts",
      "import": "./dist/utils/*.js"
    },
