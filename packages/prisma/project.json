{
  "name": "@effectify/prisma",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/prisma/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "packages/prisma/dist",
        "main": "packages/prisma/src/cli.tsx",
        "tsConfig": "packages/prisma/tsconfig.lib.json",
        "format": ["esm"],
        "generatePackageJson": false,
        "updateBuildableProjectDepsInPackageJson": false,
        "additionalEntryPoints": [],
        "assets": [
          {
            "glob": "**/*.eta",
            "input": "packages/prisma/src/templates",
            "output": "src/templates"
          }
        ]
      },
      "configurations": {
        "development": {},
        "production": {}
      }
    },
    "prisma:generate": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm dlx prisma generate",
        "cwd": "packages/prisma"
      }
    },
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsx src/messaround.ts",
        "cwd": "packages/prisma"
      }
    },
    "cli": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm dlx tsx src/cli.tsx",
        "cwd": "packages/prisma"
      }
    },
    "lint": {
      "executor": "nx-oxlint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/prisma/**/*.{ts,tsx,js,jsx}"]
      }
    }
  },
  "tags": ["node", "react", "prisma"]
}
